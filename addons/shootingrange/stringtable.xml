<?xml version="1.0" encoding="utf-8"?>
<Project name="TAC">
    <Package name="ShootingRange">
        <Key ID="STR_TAC_ShootingRange_Range">
            <English>Shooting Range</English>
        </Key>
        <Key ID="STR_TAC_ShootingRange_ModuleDesc">
            <English>This module allows you to set up a Shooting Range.</English>
        </Key>
        <Key ID="STR_TAC_ShootingRange_Name">
            <English>Name</English>
        </Key>
        <Key ID="STR_TAC_ShootingRange_NameDesc">
            <English>Name of the current range.</English>
        </Key>
        <Key ID="STR_TAC_ShootingRange_Targets">
            <English>Targets</English>
        </Key>
        <Key ID="STR_TAC_ShootingRange_TargetsDesc">
            <English>List of object names used as targets, separated by commas. Can be synchronized objects. (Objects must support animation 'terc'!)</English>
        </Key>
        <Key ID="STR_TAC_ShootingRange_Controllers">
            <English>Controllers</English>
        </Key>
        <Key ID="STR_TAC_ShootingRange_ControllersDesc">
            <English>List of object names used as controllers, separated by commas.</English>
        </Key>
        <Key ID="STR_TAC_ShootingRange_Mode">
            <English>Mode</English>
        </Key>
        <Key ID="STR_TAC_ShootingRange_ModeDesc">
            <English>Time and Hit require Duration, Trigger requires Trigger. Default: Time</English>
        </Key>
        <Key ID="STR_TAC_ShootingRange_Durations">
            <English>Durations</English>
        </Key>
        <Key ID="STR_TAC_ShootingRange_DurationsDesc">
            <English>List of available shooting durations in seconds, separated by commas. Only used in Time and Hit (Time Limited) Modes. Default: 0, 30, 60, 150, 300</English>
        </Key>
        <Key ID="STR_TAC_ShootingRange_DefaultDuration">
            <English>Default Duration</English>
        </Key>
        <Key ID="STR_TAC_ShootingRange_DefaultDurationDesc">
            <English>Default shooting duration in seconds. Only used in Time and Hit (Time Limited) Modes. Default: 60</English>
        </Key>
        <Key ID="STR_TAC_ShootingRange_TargetAmounts">
            <English>Target Amounts</English>
        </Key>
        <Key ID="STR_TAC_ShootingRange_TargetAmountsDesc">
            <English>List of available number of target pop-ups, separated by commas. Only used in Hit (Target Limited) Mode. Default: 5, 10, 15, 30, 45</English>
        </Key>
        <Key ID="STR_TAC_ShootingRange_DefaultTargetAmount">
            <English>Default Target Amount</English>
        </Key>
        <Key ID="STR_TAC_ShootingRange_DefaultTargetAmountDesc">
            <English>Default amount of target pop-ups. Only used in Hit (Target Limited) Mode. Default: 15</English>
        </Key>
        <Key ID="STR_TAC_ShootingRange_PauseDurations">
            <English>Pause Durations</English>
        </Key>
        <Key ID="STR_TAC_ShootingRange_PauseDurationsDesc">
            <English>List of available pause durations during target pop-ups in seconds, separated by commas. Only used in Timed Mode. Default: 1, 2, 3, 4, 5</English>
        </Key>
        <Key ID="STR_TAC_ShootingRange_DefaultPauseDuration">
            <English>Default Pause Duration</English>
        </Key>
        <Key ID="STR_TAC_ShootingRange_DefaultPauseDurationDesc">
            <English>Default pause duration during target pop-ups in seconds. Default: 3</English>
        </Key>
        <Key ID="STR_TAC_ShootingRange_CountdownTimes">
            <English>Countdown Times</English>
        </Key>
        <Key ID="STR_TAC_ShootingRange_CountdownTimesDesc">
            <English>List of times it takes from pressing start to first target pop-up. Default: 6, 9, 12, 15</English>
        </Key>
        <Key ID="STR_TAC_ShootingRange_DefaultCountdownTime">
            <English>Default Countdown Time</English>
        </Key>
        <Key ID="STR_TAC_ShootingRange_DefaultCountdownTimeDesc">
            <English>Default time it takes from pressing start to first target pop-up. Default: 9</English>
        </Key>
        <Key ID="STR_TAC_ShootingRange_TriggerMarkers">
            <English>Trigger Markers</English>
        </Key>
        <Key ID="STR_TAC_ShootingRange_TriggerMarkersDesc">
            <English>List of trigger markers used for controlled target pop-ups, separated by commas. Must be in order with targets. Available in Trigger Mode only.</English>
        </Key>
